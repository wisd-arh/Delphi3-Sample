#------------------------------------------------------------------------------
VERSION = BWS.01
#------------------------------------------------------------------------------
!ifndef ROOT
ROOT = $(MAKEDIR)\..
!endif
#------------------------------------------------------------------------------
MAKE = $(ROOT)\bin\make.exe -$(MAKEFLAGS) -f$**
DCC = $(ROOT)\bin\dcc32.exe $**
BRCC = $(ROOT)\bin\brcc32.exe $**
#------------------------------------------------------------------------------
PROJECTS = AskDll.dll AskChart.exe AXDll.dll
#------------------------------------------------------------------------------
default: $(PROJECTS)
#------------------------------------------------------------------------------

AskDll.dll: ..\AskDll\AskDll.dpr
  $(DCC)

AskChart.exe: AskChart.dpr
  $(DCC)

AXDll.dll: ..\AXDll\AXDll.dpr
  $(DCC)


